# Supabase
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_KEY=your_supabase_service_key

# LLM APIs
# OpenRouter - Single API key for multiple models
OPENROUTER_API_KEY=your_openrouter_api_key
# Optional: Set a specific site URL and name for OpenRouter analytics
OPENROUTER_SITE_URL=http://localhost:3000
OPENROUTER_SITE_NAME=Diverge

# Sentry Error Tracking (Optional)
NEXT_PUBLIC_SENTRY_DSN=your_sentry_dsn
SENTRY_ORG=your_sentry_organization_slug
SENTRY_PROJECT=your_sentry_project_slug
SENTRY_AUTH_TOKEN=your_sentry_auth_token

# Other Monitoring (Optional)
GRAFANA_API_KEY=your_grafana_api_key
SLACK_WEBHOOK_URL=your_slack_webhook_url

# Redis for Rate Limiting (Optional)
RATE_LIMIT_REDIS_URL=your_redis_url

# Stripe (Required for billing)
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_publishable_key

# Stripe Price IDs
NEXT_PUBLIC_STRIPE_PRICE_PLUS_MONTHLY=price_1SHZMeEoUVl3grpQHd9MCcNM
NEXT_PUBLIC_STRIPE_PRICE_PLUS_YEARLY=price_1SHhq4EoUVl3grpQseLnl4dy
NEXT_PUBLIC_STRIPE_PRICE_PRO_MONTHLY=price_1SHZOAEoUVl3grpQUmqTZRZC
NEXT_PUBLIC_STRIPE_PRICE_PRO_YEARLY=price_1SHmNGEoUVl3grpQgFE8TPTP

# Debug Settings (Optional)
# Enable detailed diagnostic logging for Enhanced Context system
# Set to 'true' to see ancestor node breakdown, token allocation, and summarization details
DEBUG_ENHANCED_CONTEXT=false